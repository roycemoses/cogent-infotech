package super_demo;

class Parent
{
	int pAge = 50;
	
	public void display()
	{
		System.out.println("Parent Age: " + this.pAge);
	}
}

class Child extends Parent
{
	int cAge = 25;
	
	public void display()
	{
		super.display();
		System.out.println("Child age: " + this.cAge);
	}
}

public class SuperDemo {
	public static void main(String[] args) {
		
		Parent ch = new Child();
		
		ch.display();
		
	}
}
