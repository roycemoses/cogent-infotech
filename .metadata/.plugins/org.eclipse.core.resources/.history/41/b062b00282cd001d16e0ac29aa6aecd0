package serialization;

public class Person {

	private int rollNum;
	private String name;
	private int age;
	private String address;
	
	public Person(int rollNum, String name, int age, String address) {
		this.rollNum = rollNum;
		this.name = name;
		this.age = age;
		this.address = address;
	}
	
	
	
	
	
	
}
