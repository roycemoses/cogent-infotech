package multithreading;

class MyThread implements Runnable
{
	public void run()
	{
		for (int i = 0; i <= 10; i++)
		{
			System.out.println("i: " + i);
			Thread.sleep(1000);
		}
	}
}

public class Main {
	
	public static void main(String[] args) {
		
		MyThread mt = new MyThread();
		Thread th = new Thread(mt);
		th.start();
	}
	
}
