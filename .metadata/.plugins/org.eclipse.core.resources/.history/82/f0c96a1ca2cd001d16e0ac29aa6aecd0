		long start = System.currentTimeMillis();
		System.out.println("Enter an integer (n): ");
		System.out.println("Sum of 'n' numbers: ");
		Scanner sc = new Scanner(System.in);
		
		JoinDemo.n = sc.nextInt();
		
		JoinDemo jd = new JoinDemo();
		jd.start();
		
		
	}
	
}
