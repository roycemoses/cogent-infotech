package gl.itp.hyd65.cs65.Royce;

public class Advisor {

	String[] message;
	
	Advisor()
	{
		message = new String[5];
	}
	
	String public getAdvice()
	{
		Random rand = new Random();
		int option = rand.nextInt(5) + 1;
		switch (option)
		{
			case 1:
				return String[0]
				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
			case 5:
				break;
				
		}
	}
	
	
}
