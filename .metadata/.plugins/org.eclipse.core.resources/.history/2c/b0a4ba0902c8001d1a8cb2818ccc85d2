
import java.util.Random;

public class ArrayBingo {
	public static void main(String[] args) {
	
		if (args.length != 2)
		{
			System.out.println("Please enter two integers!");
		}
		else
		{
			int firstNum = Integer.parseInt(args[0]);
			int secondNum = Integer.parseInt(args[1]);
			if (firstNum < 1 || firstNum > 40 || secondNum < 1 || secondNum > 40)
			{
				
			}
			
			int array[] = {5, 10, 15, 30, 40};
			
			System.out.println("First number: " + args[0]);
			System.out.println("Second number: " + args[1]);
			
			int correct = 0;
			
			for(int i:array)
			{
				if (Integer.parseInt(args[0]) == i || Integer.parseInt(args[1]) == i)
					correct++;
			}
			
			if (correct == 2)
				System.out.println("It's Bingo!");
			else
				System.out.println("Better luck next time!");
		}
		
		
		
	}
}
