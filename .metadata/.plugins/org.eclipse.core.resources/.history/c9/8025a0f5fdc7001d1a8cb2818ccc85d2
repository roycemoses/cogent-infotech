
public class ArrayLesson {
	public static void main(String[] args) {
		// invalid syntax
//		int arr[5];
		
		int arr[] = {10, 20, 30, 40 , 50};
		
		for(int i = 0; i < arr.length; i++)
		{
			System.out.println(arr[i]);
		}
		
		int array[] = new int[5];
		array[0] = 100;
		array[1] = 200;
		array[2] = 300;
		
	}
}
