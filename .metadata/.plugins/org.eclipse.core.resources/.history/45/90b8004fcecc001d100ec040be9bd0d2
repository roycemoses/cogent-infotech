package exceptions_calc;

public class UserDefinedExceptions {

	class NegativeAmountException extends Exception {

		public NegativeAmountException(String s)
		{
			super(s);
		}

	}
	
	class InsufficientFundsException extends Exception {

		public NegativeAmountException(String s)
		{
			super(s);
		}

	}
	
	class InsufficientFundsException extends Exception {

		public NegativeAmountException(String s)
		{
			super(s);
		}

	}
	
}
